{
  "expo": {
    "name": "Expo Burger King",
    "description": "Burger King: UI Clone with React Native & Expo",
    "scheme": "burgerkingclone",
    "slug": "burgerkingclone",
    "privacy": "public",
    "version": "0.0.1",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/calebnance/expo-burger-king",
    "orientation": "portrait",
    "primaryColor": "#f5ebdc",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#f5ebdc",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "versionCode": 1
    },
    "ios": {
      "buildNumber": "0.0.1",
      "supportsTablet": true
    }
  }
}
